<latex-image-preamble><![CDATA[

\usepackage{pgfplots}

\usetikzlibrary{positioning,matrix,arrows}

\usetikzlibrary{shapes,decorations,shadows,fadings}

\setlength{\marginparwidth}{215pt} 

\usepackage{ifthen}

\newboolean{colorprint}
\setboolean{colorprint}{true} 

\pgfplotsset{firstcurvestyle/.style={color=blue,mark=none,thick,<->,solid}}
\pgfplotsset{secondcurvestyle/.style={color=red,mark=none,thick,<->,dashdotted}}
\pgfplotsset{thirdcurvestyle/.style={color=magenta,mark=none,thick,<->,dashdotdotted}}
\pgfplotsset{asymptote/.style={color=black,mark=none,thick,<->,dashed}}
\pgfplotsset{guideline/.style={color=black,mark=none,-}}
\pgfplotscreateplotcyclelist{curvestylelist}{%
        firstcurvestyle\\%
        secondcurvestyle\\%
        thirdcurvestyle\\%
}

%To disable arrows, edit these and the above lines accordingly
\pgfplotsset{leftarrow/.style={<-}}
\pgfplotsset{rightarrow/.style={->}}



% Redefine these to be empty to turn off axis discontinuities
\pgfplotsset{xdiscontinuity/.style={axis x discontinuity=parallel}}
\pgfplotsset{ydiscontinuity/.style={axis y discontinuity=crunch}}

\pgfplotsset{hollowdot/.style={color=black,fill=white,only marks,mark=*}}
\pgfplotsset{soliddot/.style={color=black,fill=black,only marks,mark=*}}

\pgfplotsset{every axis/.append style = {
    cycle list name = curvestylelist,
    tick label style = {font = \scriptsize},
    axis x line = middle,
    axis y line = middle,
    xlabel = {$x$},
    ylabel = {$y$},
    x label style = {font = \scriptsize},
    y label style = {font = \scriptsize},
    minor x tick num = 1,
    minor y tick num = 1,
    width = {ifthenelse(.9\linewidth>240pt,240pt,.9\linewidth)},
}}

]]></latex-image-preamble>

